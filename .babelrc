{
    "presets": [
      ["@babel/preset-typescript", { "isTSX": true, "allExtensions": true }],
      "@babel/preset-react",
      [
        "@babel/preset-env",
        {
          "targets": { "esmodules": true },
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ]
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "displayName": true
        }
      ]
    ]
  }